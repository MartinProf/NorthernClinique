<Window x:Class="NorthernClinique.adminPersonnel"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NorthernClinique"
        mc:Ignorable="d"
        Title="Menu Personnel" Height="450" Width="800" Loaded="Window_Loaded">
    <Grid>
        <Menu HorizontalAlignment = "Center" VerticalAlignment = "Top" Width = "800">
            <MenuItem Header = "Option">
                <MenuItem x:Name="menuIAjouter" Header = "Ajouter" HorizontalAlignment = "Left" Width = "140" Click = "MenuAjouter_Click" />
                <MenuItem x:Name="menuIModifier" Header = "Modifier" HorizontalAlignment = "Left" Width = "140" Click = "MenuModifier_Click" />
                <MenuItem x:Name="menuISupprimer" Header = "Supprimer" HorizontalAlignment = "Left" Width = "140" Click = "MenuSupprimer_Click" />
            </MenuItem>
            <MenuItem Header = "Quitter">
                <MenuItem Header = "Item 1" HorizontalAlignment = "Left" Width = "140" Click = "MenuQuitter_Click" />
            </MenuItem>
        </Menu>
        <Frame x:Name="framePerso" Content="" HorizontalAlignment="Center" Height="416" Margin="0,18,0,0" VerticalAlignment="Top" Width="800"/>

    </Grid>
</Window>
